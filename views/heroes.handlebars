<div>
  <div class="row">
    <div class="col-xs-1">
    </div>
    <div class="col-xs-10">
      {{!-- Breadcrumbs --}}
      <div class="breadcrumbs">
        <h2>Heroes</h2>
        <ol class="breadcrumb">
          <li>
            <a href="/" class="breadcrumbs-a">Home</a>
          </li>
          <li>Game Info &ndash; Heroes</li>
        </ol>
      </div>
      {{!-- End Breadcrumbs --}}
    </div>
    <div class="col-xs-1">
    </div>
  </div>
  <div class="row">
    <div class="col-xs-1">
    </div>
    <div class="col-xs-10">
      <div class="jumbotron">
        <div class="row">
          <div class="col-xs-12">
            <div class="content">
              <div class="jumbotron" id="jumbotron8">
                <div class="navbar-inverse" id="login-header">
                  <div class="row">
                    <div class="col-xs-8" id="paddingtop3">
                      Champion Filters
                    </div>
                    <div class="col-xs-4">
                      <div class="input-group">
                        <input type="text" class="form-control search" id="search" placeholder="Search Heroes">
                        <span class="input-group-btn">
                          <button class="btn btn-default" type="button" id="search">
                            <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
                          </button>
                        </span>
                      </div><!-- /input-group -->
                    </div><!-- /.col-lg-6 -->
                  </div>
                </div>
                <div class="checkClass">
                  <p>
                    Classes:
                  </p>
                </div>
                  <div class="checkClass category_filters">
                    <div class="checkbox checkbox-primary checkbox-inline">
                      <input type="checkbox" class="category_item" id="filter-tank" value="tank">
                      <label for="filter-tank">Tank</label>
                    </div>
                    <div class="checkbox checkbox-primary checkbox-inline">
                      <input type="checkbox" class="category_item" id="filter-fighter" value="fighter">
                      <label for="filter-fighter">Fighter</label>
                    </div>
                    <div class="checkbox checkbox-primary checkbox-inline">
                      <input type="checkbox" class="category_item" id="filter-mage" value="mage">
                      <label for="filter-mage">Mage</label>
                    </div>
                    <div class="checkbox checkbox-primary checkbox-inline">
                      <input type="checkbox" class="category_item" id="filter-marksman" value="marksman">
                      <label for="filter-marksman">Marksman</label>
                    </div>
                    <div class="checkbox checkbox-primary checkbox-inline">
                      <input type="checkbox" class="category_item" id="filter-support" value="support">
                      <label for="filter-support">Support</label>
                    </div>
                  </div>
              </div>
            </div>
          </div>
          <div class="col-xs-12 paddingtop0">
            <div class="content">
              <div class="jumbotron" id="jumbotron8">
                <div class="navbar-inverse" id="login-header">
                  <div class="row">
                    <div class="col-xs-8" id="paddingtop3">
                      Champion Grid
                    </div>
                  </div>
                </div>
                {{# each heroes }}
                  <div class="row block_container" id="heroesGrid">
                    {{# each this }}
                      <div class="col-xs-6 col-sm-4 col-md-2" id="section">
                        <div class="hero {{ toLowerCase this.class }}">
                          <div class="grid">
                            <a href="/heroes/{{ this.urlName }}">
                              <img src="{{ this.icon_path }}" alt="{{ this.name }} Icon" class="img-responsive img-thumbnail" id="{{ this.class }}"></a>
                          </div>
                          <div class="caption">
                            <p class="sticker-title">{{ this.name }}</p>
                          </div>
                        </div>
                      </div>
                    {{/each}}
                    <div class="noHeroes">
                      <div class="" id="jumbotron8">
                        <p>No heroes match your criteria.</p>
                      </div>
                    </div>
                  </div>
                {{/each}}
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="col-xs-1">
    </div>
  </div>
</div>
