                        <div class="">
                          <img src="{{ skill1_icon }}" alt="{{ skill1_title }}" class="img-skill">
                        </div>
                      </div>
                      <div class="col-xs-12 col-sm-9 paddingtop0">
                        <div class="skill1-title">
                          {{ skill1_title }}
                        </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-xs-12 paddingtop0">
                        <div class="spell-description">
