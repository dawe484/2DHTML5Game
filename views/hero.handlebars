                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="col-xs-6">
                    <div class="video">

                    </div>
                  </div>
                </div>
                <!-- Skill Divider -->
                <div class="row">
                  <div class="grid paddingright20">
                    <hr class="spell-divider">
                  </div>
                </div>
                <!-- End Skill 3 + Divider -->
                <!-- Skill 4 -->
                <div class="row caption">
                  <div class="col-xs-6">
                    <div class="row">
                      <div class="col-xs-12 col-sm-3 caption paddingtop0">
                        <div class="">
                          <img src="{{ skill4_icon }}" alt="{{ skill4_title }}" class="img-skill">
                        </div>
                      </div>
                      <div class="col-xs-12 col-sm-9 paddingtop0">
                        <div class="skill1-title">
                          {{ skill4_title }}
                        </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-xs-12 paddingtop0">
                        <div class="spell-description">
                          {{ skill4_description }}
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="col-xs-6">
                    <div class="video">

                    </div>
                  </div>
                </div>
                <!-- End Skill 4-->
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="col-xs-1">
    </div>
  </div>
</div>
