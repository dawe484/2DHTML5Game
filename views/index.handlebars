		<!-- Login Content -->
		<div class="col-xs-12 col-md-4">

			{{#if user}}

			<div class="content-login">
				<div class="jumbotron" id="jumbotron4">
					<div class="navbar-inverse" id="login-header">
						LOGIN
					</div>
					<div class="my-form">
						<div class="col-md-12" id="logged">
							<a href="" type="button" class="btn btn-default">Play now</a>
						</div>
						<label></label>
					</div>
				</div>
			</div>

			{{else}}

			<div class="content-login">
				<div class="jumbotron" id="jumbotron4">
