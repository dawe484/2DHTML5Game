<!-- Carousel -->
<div id="indexCarousel" class="carousel fade" data-ride="carousel">
  <!-- Indicators -->
	<ol class="carousel-indicators">
    <li data-target="#indexCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#indexCarousel" data-slide-to="1" ></li>
    <li data-target="#indexCarousel" data-slide-to="2" ></li>
	</ol>
	<div class="carousel-inner" role="listbox">
    <div class="item active">
      <a href="#" onclick="_gaq.push(['_trackEvent','v2_home','banner_0','lc'])">
        <span class="img-holder third-slide" style="background-image:url('img/MH_index-1680-380_01.jpg')">
        </span>
      </a>
    </div>
    <div class="item ">
      <a href="#" onclick="_gaq.push(['_trackEvent','v2_home','banner_1','rom'])">
        <span class="img-holder third-slide" style="background-image:url('http://img1.loygame.com/attach/2016_09_14_07_54_10_951_banner-1680-380-ROM.png')">
        </span>
      </a>
    </div>
    <div class="item ">
      <a href="#" onclick="_gaq.push(['_trackEvent','v2_home','banner_2','soh'])">
        <span class="img-holder third-slide" style="background-image:url('http://img1.loygame.com/attach/2016_09_28_05_33_32_760_SOH-S20-1680-380.jpg')">
        </span>
      </a>
    </div>
	</div>
</div>
<!-- End Carousel -->

<div class="jumbotron" id="jumbo">
	<div class="row">
		<div class="col-xs-12 col-md-8">
			<!-- First Content -->
			<div class="content">
				<div class="jumbotron" id="jumbotron8">
					<a href="">
						<div class="navbar-inverse" id="news-header">
							New Website Design
						</div>
					</a>
					<div class="news-date">
						<p>from <cite>The Magical Heroes Team</cite> today at 11:09 AM</p>
